{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {
                "no-empty-function": "off",
                "@typescript-eslint/no-empty-function": "error"
            }
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {
                "no-empty-function": "off",
                "@typescript-eslint/no-empty-function": "error"
            }
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {
                "no-empty-function": "off",
                "@typescript-eslint/no-empty-function": "error"
            }
        }
    ]
}
